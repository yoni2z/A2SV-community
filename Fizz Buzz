class Solution:
    def fizzBuzz(self, n: int) -> List[str]:
        fizbuz = []

        for i in range(1, n + 1):
            if (i % 5 == 0 and i % 3 == 0):
                fizbuz.append("FizzBuzz")
            elif (i % 3 == 0):
                fizbuz.append("Fizz")
            elif (i % 5 == 0):
                fizbuz.append("Buzz")
            else:
                fizbuz.append(str(i))

        return fizbuz
        
        
